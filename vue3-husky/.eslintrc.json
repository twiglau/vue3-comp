{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "standard-with-typescript"
    ],
    "parser": "vue3",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": true,
        "project": "./tsconfig.json"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": ["warn","never"]
    }
}
